datasource db {
    provider = "postgresql"
}

generator client {
    provider = "prisma-client"
    output   = "./generated"
}

model User {
    id          Int      @id @default(autoincrement())
    email       String?  @unique
    phone       String?  @unique
    countryCode String?
    password    String?
    name        String?
    createdAt   DateTime @default(now())
}
